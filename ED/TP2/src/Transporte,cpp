#include "Transporte.hpp"

Transporte::Transporte()
    : idPacote(-1), latenciaTransporte(0), armazemOrigem(-1),
      armazemDestino(-1), capacidadeTransporte(0) {}

Transporte::Transporte(int id, int latencia, int origem, int destino, int capacidade)
    : idPacote(id), latenciaTransporte(latencia), armazemOrigem(origem),
      armazemDestino(destino), capacidadeTransporte(capacidade) {}

Transporte::~Transporte() {}

int Transporte::getIdPacote() const {
    return idPacote;
}

int Transporte::getLatenciaTransporte() const {
    return latenciaTransporte;
}

int Transporte::getArmazemOrigem() const {
    return armazemOrigem;
}

int Transporte::getArmazemDestino() const {
    return armazemDestino;
}

int Transporte::getCapacidadeTransporte() const {
    return capacidadeTransporte;
}